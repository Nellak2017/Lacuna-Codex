{
  "name": "Lacuna-Codex",
  "version": "0.1.0",
  "description": "Lacuna-Codex",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-count": "next lint 2>&1 | grep -Ei \"error|warning\" | wc -l | xargs -I {} echo \"{} problems\"",
    "line-count-sorted": "find ./src/ -type f \\( -name '*.js' -o -name '*.mjs' \\) -exec wc -l {} + | sort -n",
    "line-count-sorted-no-fluff": "find ./src/ -type f \\( -name '*.js' -o -name '*.mjs' -o -name '*.ts' -o -name '*.tsx' \\) ! \\( -name '*stories*' -o -name '*test*' \\) -exec wc -l {} + | sort -n",
    "test": "jest",
    "failing-test": "jest --onlyFailures",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cache": "jest --clearCache",
    "clean:res": "bsb -clean-world",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/lab": "7.0.0-beta.17",
    "@mui/material": "^7.3.2",
    "@reduxjs/toolkit": "^2.9.0",
    "@supabase/supabase-js": "^2.57.2",
    "dotenv": "^17.2.2",
    "jest": "^30.1.3",
    "media-chrome": "^4.13.1",
    "next": "^15.5.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-google-button": "^0.8.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-loader-spinner": "^6.1.6",
    "react-player": "^3.3.2",
    "react-redux": "^9.2.0",
    "react-toastify": "^11.0.5",
    "react-use": "^17.6.0",
    "redux": "^5.0.1",
    "reselect": "^5.1.1",
    "wretch": "^2.11.0"
  },
  "devDependencies": {
    "@fast-check/jest": "^2.1.1",
    "@storybook/nextjs": "^9.1.4",
    "@storybook/react": "^9.1.4",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "cypress": "^15.1.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-storybook": "^9.1.4",
    "fast-check": "^4.3.0",
    "jest-environment-jsdom": "^30.1.2",
    "redux-mock-store": "^1.5.5",
    "storybook": "^9.1.4"
  }
}
